#include <stdio.h>

int main()
{
    int i,j,m,n;
    int a[10][10];
    printf("Enter the number of rows:");
    scanf("%d",&m);
    printf("Enter the number of columns:");
    scanf("%d",&n);
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("Enter the data in [%d][%d]:",i,j);
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        { 
            printf("%d\t",a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
